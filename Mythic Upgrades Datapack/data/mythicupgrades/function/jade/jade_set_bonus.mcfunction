execute as @a[tag=!mythicupgrades.jade_armor] run tag @s add mythicupgrades.jade_armor
effect give @s minecraft:jump_boost infinite 2 true
effect give @s minecraft:speed infinite 2 true
item modify entity @s armor.chest mythicupgrades:jade_set_lore
item modify entity @s armor.feet mythicupgrades:jade_set_lore
item modify entity @s armor.head mythicupgrades:jade_set_lore
item modify entity @s armor.legs mythicupgrades:jade_set_lore